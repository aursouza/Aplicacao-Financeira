{
  "name": "app-fin-backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec npm run load",
    "load": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "build": "rimraf dist &&tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "keywords": [],
  "author": "Marco Aurelio de Souza",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.5",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "pre-commit": "^1.2.2",
    "prisma": "^5.16.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "uuid": "^10.0.0"
  }
}
